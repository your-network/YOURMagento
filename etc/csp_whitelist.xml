<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="img-src">
            <values>
                <value id="your-integration-img-src-loom" type="host">*.loom.com</value>
                <value id="your-integration-img-src-wasabisys" type="host">*.wasabisys.com</value>
                <value id="your-integration-img-src-githubusercontent" type="host">*.githubusercontent.com</value>
                <value id="your-integration-img-src-zendesk" type="host">*.zendesk.com</value>
                <value id="your-integration-img-src-zdassets" type="host">*.zdassets.com</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="your-integration-script-src-jsdelivr" type="host">*.jsdelivr.net</value>
                <value id="your-integration-script-src-cloudflare" type="host">*.cloudflare.com</value>
                <value id="your-integration-script-src-zendesk" type="host">*.zendesk.com</value>
                <value id="your-integration-script-src-zdassets" type="host">*.zdassets.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="your-integration-style-src-cloudflare" type="host">*.cloudflare.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="your-integration-connect-src-your" type="host">*.your.io</value>
                <value id="your-integration-connect-src-wasabisys" type="host">*.wasabisys.com</value>
                <value id="your-integration-connect-src-zendesk" type="host">*.zendesk.com</value>
                <value id="your-integration-connect-src-pod-29.zendesk.com" type="host">wss://pod-29.zendesk.com</value>
                <value id="your-integration-connect-src-zdassets" type="host">*.zdassets.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>

